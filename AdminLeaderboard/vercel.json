{
  "buildCommand": "cd AdminLeaderboard && npm run build",
  "devCommand": "cd AdminLeaderboard && npm run dev",
  "installCommand": "cd AdminLeaderboard && npm install",
  "outputDirectory": "AdminLeaderboard/dist/public",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
